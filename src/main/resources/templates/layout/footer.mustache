<script>
    DecoupledEditor
        .create( document.querySelector( '#content' ),{
            ckfinder: {
                    uploadUrl: '/api/v1/image/upload'
                }
        })
        .then(  newEditor => {
            editor = newEditor;

            const toolbarContainer = document.querySelector( '#toolbar-container' );

            toolbarContainer.appendChild( editor.ui.view.toolbar.element );
        } );
